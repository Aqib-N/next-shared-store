{
  "name": "next-shared-state",
  "version": "2.0.0",
  "description": "Enhanced state sharing for Next.js with IndexedDB persistence and URL data transfer between pages and components.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nextjs",
    "react",
    "state-management",
    "shared-state",
    "persistence",
    "indexeddb",
    "url-transfer",
    "static-dynamic",
    "page-state",
    "cross-page"
  ],
  "author": "Aqib Nawaz <aqibalipk777@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Aqib-N/next-shared-store",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aqib-N/next-shared-store.git"
  },
  "bugs": {
    "url": "https://github.com/Aqib-N/next-shared-store/issues"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  }
}
